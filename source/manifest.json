{
  "manifest_version": 2,

  "name": "Stop Wasting Time",
  "description": "Hides the newsfeed and notification button on facebook website",
  "version": "1.1",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "js": ["stopWastingTime.js"],
      "run_at": "document_start"
    }
  ]
}